{
    "name": "silverstripe/saml",
    "description": "Adds SSO/SAML authentication support to SilverStripe",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "ad",
        "active",
        "directory",
        "sso",
        "saml"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Sean Harvey",
            "email": "sean@silverstripe.com"
        },
        {
            "name": "Stig Lindqvist",
            "email": "stig@silverstripe.com"
        }
    ],
    "require": {
        "silverstripe/framework": "^6",
        "onelogin/php-saml": "^4.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "3.0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\SAML\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SilverStripe\\SAML\\Tests\\": "tests/php/"
        }
    },
    "suggest": {
        "silverstripe/ldap": "Synchronise extra user attributes from Active Directory with LDAP"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    }
}
