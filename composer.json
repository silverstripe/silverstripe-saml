{
    "name": "silverstripe/saml",
    "description": "Adds SSO/SAML authentication support to SilverStripe",
    "type": "silverstripe-vendormodule",
    "keywords": ["silverstripe", "ad", "active", "directory", "sso", "saml"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Sean Harvey",
            "email": "sean@silverstripe.com"
        },
        {
            "name": "Stig Lindqvist",
            "email": "stig@silverstripe.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "silverstripe/framework": "^4@dev",
        "silverstripe/cms": "^4@dev",
        "zendframework/zend-authentication": "^2.5.1",
        "zendframework/zend-session": "^2.5.1",
        "onelogin/php-saml": "^3.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\SAML\\": "src/",
            "SilverStripe\\SAML\\Tests\\": "tests/php/"
        }
    },
    "suggest": {
        "silverstripe/ldap": "Synchronise extra user attributes from Active Directory with LDAP"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
